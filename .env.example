# Environment Configuration
NODE_ENV=development
PORT=4000

GOOGLE_CLIENT_ID=xxxx.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=xxxx
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback
# Database Configuration
DATABASE_URL=xxx

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-at-least-32-characters-long
JWT_ISS=xxx
JWT_AUD=xxx
TOKEN_TTL=1h
REFRESH_TTL=30d

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password
SMTP_FROM="Healing App" <noreply@healing.com>

# MoMo Payment Configuration
MOMO_PARTNER_CODE=MOMOPAYMENT2024
MOMO_ACCESS_KEY=your-momo-access-key
MOMO_SECRET_KEY=your-momo-secret-key
MOMO_ENDPOINT_CREATE=https://test-payment.momo.vn/v2/gateway/api/create
MOMO_ENDPOINT_CONFIRM=https://test-payment.momo.vn/v2/gateway/api/confirm
MOMO_REDIRECT_URL=http://localhost:3000/payment/result
MOMO_IPN_URL=http://localhost:4000/api/v1/payments/momo/ipn

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Logging Configuration
LOG_LEVEL=info
LOG_FILE_PATH=logs/

# Cache Configuration
CACHE_TTL=300000

# File Upload Configuration
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp,application/pdf

# Security Configuration
BCRYPT_ROUNDS=10
SESSION_SECRET=your-session-secret-key

# API Configuration
API_VERSION=v1
API_PREFIX=/api

# WebSocket Configuration
SOCKET_CORS_ORIGIN=http://localhost:3000
SOCKET_TIMEOUT=30000

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30000
HEALTH_CHECK_TIMEOUT=5000

# Monitoring Configuration
ENABLE_METRICS=true
METRICS_PORT=9090

# Database Pool Configuration
DB_POOL_MAX=20
DB_POOL_MIN=2
DB_POOL_IDLE_TIMEOUT=30000
DB_POOL_CONNECTION_TIMEOUT_MS=2000

# Redis Configuration (if using Redis cache)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# Third-party API Keys
SENTRY_DSN=your-sentry-dsn
NEW_RELIC_LICENSE_KEY=your-new-relic-license-key
GOOGLE_ANALYTICS_ID=your-google-analytics-id

# Feature Flags
ENABLE_EMAIL_VERIFICATION=true
ENABLE_SOCIAL_LOGIN=false
ENABLE_TWO_FACTOR_AUTH=false
ENABLE_RATE_LIMITING=true
ENABLE_CACHING=true
ENABLE_REQUEST_LOGGING=true
ENABLE_ERROR_TRACKING=true

# Development Configuration
ENABLE_DEBUG_MODE=true
ENABLE_MOCK_EMAIL=true
ENABLE_MOCK_PAYMENT=false

# Production Configuration
ENABLE_COMPRESSION=true
ENABLE_HTTP2=false
ENABLE_SSL=false
SSL_CERT_PATH=
SSL_KEY_PATH=

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_INTERVAL=86400
BACKUP_RETENTION_DAYS=30
BACKUP_STORAGE_PATH=backups/

# Analytics Configuration
ENABLE_ANALYTICS=true
ANALYTICS_BATCH_SIZE=100
ANALYTICS_FLUSH_INTERVAL=30000


# AI
GEMINI_API_KEY = xxxxx

